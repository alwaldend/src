load("//tools/docs:al_docs_files.bzl", "al_docs_files")

alias(
    name = "shellcheck",
    actual = "@com_alwaldend_src_tools_stackage_exe//ShellCheck:shellcheck",
    visibility = ["//:__subpackages__"],
)

al_docs_files(
    name = "docs",
    srcs = glob(["*.md"]),
    prefix = package_name(),
    visibility = ["//tools:__pkg__"],
    deps = ["//tools/shellcheck/main/bzl:docs"],
)
