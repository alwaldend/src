load("//tools/docs/main/bzl:al_docs_files.bzl", "al_docs_files")

alias(
    name = "install_file",
    actual = "//tools/install_file/main/py:install_file",
    visibility = ["//:__subpackages__"],
)

al_docs_files(
    name = "docs",
    srcs = glob(["*.md"]),
    prefix = package_name(),
    visibility = ["//tools:__pkg__"],
    deps = [
        "//tools/install_file/main/bzl:docs",
    ],
)
